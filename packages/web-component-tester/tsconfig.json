{
    "extends": "../../tsconfig-base.json",
    "compilerOptions": {
        "noUnusedLocals": false,
        "noImplicitThis": false,
        "strictNullChecks": false,
        "suppressImplicitAnyIndexErrors": true,
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "browser/*.ts",
        "runner/*.ts",
        "test/**/*.ts",
        "custom_typings/*.d.ts",
        "node_modules/@types/mocha/index.d.ts",
        "node_modules/wct-mocha/**/*.d.ts"
    ]
}