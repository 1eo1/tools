{
  "name": "hydrolysis",
  "version": "1.21.4",
  "description": "Breaks polymers into monomers",
  "homepage": "https://github.com/Polymer/hydrolysis",
  "bugs": "https://github.com/Polymer/hydrolysis/issues",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Polymer/hydrolysis.git"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE",
    "lib/"
  ],
  "scripts": {
    "init": "tsd install",
    "clean": "rm -rf node_modules/ typings/ *.js && find lib | grep \\.js$ | xargs rm",
    "build": "tsc && browserify -d -r ./index.js:hydrolysis -o hydrolysis.js -t babelify",
    "build:watch": "watch 'npm run build' ./lib",
    "release": "browserify -r ./index.js:hydrolysis -o hydrolysis.js -t babelify",
    "apidocs": "node_modules/jsdoc-to-markdown/bin/cli.js {index.js,lib/{analyzer,loader/*}.js} > API.md",
    "test": "npm run build && wct && mocha test/test.js",
    "testNode": "npm run build && mocha test/test.js"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^9.0.8",
    "chai": "^2.2.0",
    "jsdoc-to-markdown": "^1.0.3",
    "mocha": "^2.2.4",
    "tsd": "^0.6.5",
    "typescript": "^1.8.0-dev.20160125",
    "watch": "latest",
    "web-component-tester": "^3.3.21"
  },
  "dependencies": {
    "babel-polyfill": "^6.2.0",
    "doctrine": "^0.7.0",
    "dom5": "^1.1.0",
    "escodegen": "^1.7.0",
    "espree": "^2.0.1",
    "estraverse": "^3.1.0",
    "path-is-absolute": "^1.0.0"
  }
}
