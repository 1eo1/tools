{
  "name": "hydrolysis",
  "version": "1.24.1",
  "description": "Breaks polymers into monomers",
  "homepage": "https://github.com/Polymer/hydrolysis",
  "bugs": "https://github.com/Polymer/hydrolysis/issues",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/Polymer/hydrolysis.git"
  },
  "main": "index.js",
  "files": [
    "hydrolysis-analyzer.html",
    "hydrolysis.html",
    "hydrolysis.js",
    "index.js",
    "LICENSE",
    "lib/"
  ],
  "scripts": {
    "init": "typings install",
    "clean": "rm -rf typings/ lib/ && find src | grep \\.js$ | xargs rm",
    "compile": "if [ ! -f typings/index.d.ts ]; then npm run init; fi; tsc",
    "build": "npm run compile && browserify -d -r ./index.js:hydrolysis -o hydrolysis.js",
    "release": "npm run compile && browserify -r ./index.js:hydrolysis -o hydrolysis.js",
    "build:watch": "watch 'npm run build' ./src",
    "apidocs": "node_modules/jsdoc-to-markdown/bin/cli.js {index.js,lib/{analyzer,loader/*}.js} > API.md",
    "test": "npm run build && wct && mocha",
    "testNode": "npm run build && mocha"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "chai": "^2.2.0",
    "depcheck": "^0.6.3",
    "fs-extra": "^0.30.0",
    "gulp-eslint": "^2.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.4",
    "gulp-typings": "^1.3.6",
    "jsdoc-to-markdown": "^1.0.3",
    "mocha": "^2.2.4",
    "run-sequence": "^1.2.0",
    "sinon": "^1.17.4",
    "temp": "^0.8.3",
    "tslint": "^3.10.2",
    "typescript": "^1.8.0",
    "typings": "^1.3.1",
    "watch": "latest",
    "web-component-tester": "^4.2.2"
  },
  "dependencies": {
    "acorn": "^3.2.0",
    "doctrine": "^0.7.0",
    "dom5": "^1.1.0",
    "escodegen": "^1.7.0",
    "estraverse": "^3.1.0",
    "parse5": "^2.1.5",
    "path-is-absolute": "^1.0.0",
    "shady-css-parser": "0.0.8"
  }
}
